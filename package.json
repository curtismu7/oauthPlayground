		"react-pro-sidebar": "^1.1.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.28.0",
		"react-tabs": "^6.1.0",
		"react-toastify": "^11.0.5",
		"sqlite3": "^5.1.7",
		"styled-components": "^6.1.19",
		"tailwind-merge": "^3.3.1",
		"use-sync-external-store": "^1.2.2",
		"winston": "^3.17.0",
