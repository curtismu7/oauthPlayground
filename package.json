{
	"name": "masterflow-api",
	"version": "9.11.32",
	"mfaV8Version": "9.11.32",
	"unifiedV8uVersion": "9.11.32",
	"protectPortalVersion": "9.11.32",
	"description": "MasterFlow API - PingOne API integration and testing platform",
	"main": "src/main.tsx",
	"type": "module",
	"scripts": {
		"start": "./scripts/dev/start-full-stack.sh",
		"start:frontend": "vite",
		"start:backend": "BACKEND_PORT=3001 node server.js",
		"start:interactive": "./scripts/dev/start-interactive.sh",
		"start:dev": "./scripts/dev/start-dev.sh",
		"test:all": "npm run test:backend && npm run test && npm run test:e2e",
		"test:startup": "./tests/manual/test-startup.sh",
		"dev:enhanced": "./scripts/dev/start-dev.sh",
		"dev:status": "./scripts/dev/start-dev.sh",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "biome format --write .",
		"format:prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"lint": "biome lint src",
		"lint:eslint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
		"lint:eslint:fix": "eslint . --ext .ts,.tsx --fix",
		"lint:all": "npm run lint && npm run lint:eslint",
		"check": "biome check .",
		"fix": "biome check --write .",
		"precommit": "biome check --staged .",
		"type-check": "tsc --noEmit",
		"prepare": "husky",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:unit": "vitest run src/services/__tests__/",
		"test:integration": "vitest run src/services/__tests__/*.integration.test.ts",
		"test:contract": "vitest run src/services/__tests__/*.contract.test.ts",
		"test:password-reset": "vitest run src/services/__tests__/passwordResetService.test.ts",
		"test:password-reset:all": "vitest run src/services/__tests__/passwordResetService*.test.ts",
		"protect:password-reset": "./scripts/protect-password-reset.sh",
		"test:services": "vitest run src/services/__tests__/ --reporter=verbose",
		"test:puppeteer": "node tests/puppeteer/sidebar-menu.test.js",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:report": "playwright show-report",
		"db:seed-users": "node scripts/db-seed-users.js",
		"service-discovery": "node src/services/serviceDiscoveryCLI.js",
		"lock:mfa": "node scripts/lockdown/lock-mfa.mjs",
		"lock:mfa:dry-run": "node scripts/lockdown/lock-mfa.mjs --dry-run",
		"lock:fido2": "node scripts/lockdown/lock-fido2.mjs",
		"lock:fido2:dry-run": "node scripts/lockdown/lock-fido2.mjs --dry-run",
		"lock:mfa-hub": "node scripts/lockdown/lock-mfa-hub.mjs",
		"lock:mfa-hub:dry-run": "node scripts/lockdown/lock-mfa-hub.mjs --dry-run",
		"lock:email": "node scripts/lockdown/lock-email.mjs",
		"lock:email:dry-run": "node scripts/lockdown/lock-email.mjs --dry-run",
		"lock:whatsapp": "node scripts/lockdown/lock-whatsapp.mjs",
		"lock:whatsapp:dry-run": "node scripts/lockdown/lock-whatsapp.mjs --dry-run",
		"lock:unified-flow": "node scripts/lockdown/lock-unified-flow.mjs",
		"lock:unified-flow:dry-run": "node scripts/lockdown/lock-unified-flow.mjs --dry-run",
		"lock:device-code": "node scripts/lockdown/lock-device-code.mjs",
		"lock:device-code:dry-run": "node scripts/lockdown/lock-device-code.mjs --dry-run",
		"config": "node src/services/configurationManagerDemo.js",
		"audit:async": "node scripts/audit-async-patterns.js",
		"verify:sms-lockdown": "node scripts/lockdown/lockdown.mjs verify sms",
		"sms:lockdown:approve": "node scripts/lockdown/lockdown.mjs approve sms",
		"verify:fido2-lockdown": "node scripts/lockdown/lockdown.mjs verify fido2",
		"fido2:lockdown:approve": "node scripts/lockdown/lockdown.mjs approve fido2",
		"verify:email-lockdown": "node scripts/lockdown/lockdown.mjs verify email",
		"email:lockdown:approve": "node scripts/lockdown/lockdown.mjs approve email",
		"verify:whatsapp-lockdown": "node scripts/lockdown/lockdown.mjs verify whatsapp",
		"whatsapp:lockdown:approve": "node scripts/lockdown/lockdown.mjs approve whatsapp",
		"verify:success-page-lockdown": "node scripts/lockdown/lockdown.mjs verify success-page",
		"success-page:lockdown:approve": "node scripts/lockdown/lockdown.mjs approve success-page"
	},
	"keywords": [
		"oauth",
		"oidc",
		"pingone",
		"authentication",
		"authorization",
		"playground"
	],
	"author": "",
	"license": "MIT",
